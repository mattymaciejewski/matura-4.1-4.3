#include <iostream>
#include <cmath>
#include <iomanip>
#include <fstream>  

using namespace std;

const double PI = 3.141592653589793;

int main() {
    double r = 5.00;
    double dt = 0.05;
    double tPoczatkowe = 3.00;
    double T = 12.5;

  fstream plik;
  plik.open("wyniki.txt", ios::out);
  if(!plik.is_open()) {
    cout << "plik sie nie otwiera" << "\n";
  }

    for (double t = tPoczatkowe; t <= T; t += dt) {
        double x = r * sin(2 * PI * t / T);
        double y = r * cos(2 * PI * t / T);
    plik << x << "\t" << y << "\n";
        if (y > x) {
            cout << fixed << setprecision(2) << t - tPoczatkowe << "\n";
            break;
        }
    }

    plik.close();

    return 0;
}
